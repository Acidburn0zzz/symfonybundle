{
    "name": "dwoo/symfony-bundle",
    "license": "LGPL",
    "type": "library",
    "authors": [
        {
            "name": "David Sanchez",
            "email": "david38sanchez@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Dwoo\\SymfonyBundle\\": "./"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dwoo\\SymfonyBundle\\Tests\\": "tests/"
        }
    },
	"repositories": [
		{
			"url": "https://gitlab.com/dwooproject/dwoo.git",
			"type": "vcs"
		}
	],
    "require": {
        "php": "~5.5|~7.0",
        "symfony/framework-bundle": "~2.8|~3.0",
        "symfony/templating": "~2.8|~3.0",
        "dwoo/dwoo": "1.3.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.27"
    },
	"suggest": {
		"symfony/asset": "Symfony asset support in Dwoo templates",
		"symfony/assetic-bundle": "Assetic support in Dwoo templates"
	}
}
