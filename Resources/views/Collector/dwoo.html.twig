{% extends 'WebProfilerBundle:Profiler:layout.html.twig' %}

{% block toolbar %}
    {% set icon %}
        <img width="18" height="18" style="padding-top:6px;padding-bottom:4px;" alt="{{ (collector.name|capitalize)|default('HTTP calls') }}" src="data:image/png;base64," />
        <span class="sf-toolbar-status">{{ constant('Dwoo\\Core::VERSION') }}</span>
    {% endset %}

    {% include 'WebProfilerBundle:Profiler:toolbar_item.html.twig' with { 'link': 'dwoo' } %}
{% endblock %}

{% block menu %}
    <span class="label">
        <span class="icon"><img src="data:image/png;base64," alt="{{ (collector.name)|default('HTTP calls') }}" /></span>
        <strong>{{ (collector.name|capitalize)|default('HTTP calls') }}</strong>
    </span>
{% endblock %}

{% block panel %}
    <h2>Dwoo</h2>
    <div class="metrics">
        <div class="metric">
            <span class="value">{{ constant('Dwoo\\Core::VERSION') }}</span>
            <span class="label">Dwoo version</span>
        </div>
    </div>
{% endblock %}